{
  "name": "6tisch-simulator-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm:backend-server\" \"npm:dev-server\"",
    "build": "vue-cli-service build",
    "test": "concurrently -k -s first \"npm:backend-server\" \"npm:test:unit\"",
    "lint": "vue-cli-service lint",
    "backend-server": "backend/start --auto-restart",
    "dev-server": "vue-cli-service serve",
    "test:backend": "pytest backend",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "vue": "^2.5.17",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.3",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
